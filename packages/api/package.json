{
	"name": "@lamington/api",
	"version": "1.0.0-alpha.4",
	"private": true,
	"description": "Library of application interfaces to extend interactions with EOSIO smart contracts and Lamington",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"files": [
		"lib/**/*",
		"README.md"
	],
	"scripts": {
		"build": "tsc",
		"clean": "rm -rf ./lib",
		"docs": "typedoc --out api-docs ./src",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
		"prepublishOnly": "npm test",
		"prepare": "npm run build",
		"test": "mocha --require ts-node/register src/**/*.test.ts",
		"link": "yarn link"
	},
	"license": "MIT",
	"devDependencies": {
		"@types/chai": "4.1.7",
		"@types/mocha": "5.2.7",
		"@types/node": "12.0.10",
		"prettier": "1.18.2",
		"typedoc": "0.14.2",
		"typescript": "3.5.2"
	},
	"dependencies": {
		"chai": "4.2.0",
		"mocha": "6.1.4",
		"eosjs": "20.0.0",
		"eosjs-ecc": "4.0.4"
	},
	"peerDependencies": {
		"eosjs": "20.0.0",
		"eosjs-ecc": "4.0.4"
	}
}
